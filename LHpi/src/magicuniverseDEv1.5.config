sitedomain = "www.magicuniverse.de/html/"
sitefile = "magic.php?startrow=1"
sitesetprefix = "&edition="
sitefrucprefix = "&rarity="
sitesuffix = ""

siteRegex = '<tr>\n<td align="center">\n%b<>%b<>\n%b<>%b<>%b<>\n%b<>%b<>\n(.-)\n%b<>\n</td>\n</tr>'

function siteSortData ( foundstring )
	local _start,_end,nameE = string.find(foundstring, 'name="namee" value="([^"]+)"' )
	local _start,_end,nameG = string.find(foundstring, 'name="named" value="([^"]+)"' )
	local price = nil
	if not STAMMKUNDE then
		local _start,_end,lprice = string.find(foundstring, 'name="preis" value="([%d.,]+)"' )
		price = lprice
	else
		local _start,_end,lprice = string.find(foundstring, '<a href="rabatt.php" target="_blank"  class="linkblack"> %(([%d.,]+) &euro;%)' )
		price = lprice
	end
	local newCard = { names = { [1] = nameE , [3] = nameG }, price = price }
	if DEBUG then
		llog( "siteSortData\t " .. deeptostring(newCard) ,2)
	end
	return newCard
end -- function siteSortData

function siteCardDataManipulation ( card , setid )
	if DEBUG then
		llog ("siteCardDataManipulation got " .. deeptostring(card) .. " from " .. setid, 2)
	end
	-- seperate "(alpha)" and beta from beta-urls
	if setid == 90 then -- importing Alpha
		if string.find( card.name, "%([aA]lpha%)" ) then
			card.name = string.gsub( card.name, "%s*%([aA]lpha%)", "" )
		else -- not "(alpha")
			card.name = card.name .. "(DROP notalpha)" -- change card.name to prevent import
		end
	elseif setid == 100 then -- importing Beta
		if string.find( card.name, "%([aA]lpha%)" ) then
			card.name = card.name .. "(DROP not beta)" -- change card.name to prevent import
		else -- not "(alpha")
			card.name = string.gsub( card.name, "%s*%(beta%)", "" ) -- catch needlessly suffixed rawdata
		end 
	-- special case
	elseif setid == 140 then -- importing Revised
		if card.name == "Schilftroll (Fehldruck, deutsch)" then
			card.lang = {nil,nil,"GER"}
			card.name = "Schilftroll"
		end
	end -- if 90 elseif 100 elseif 140

	if setid == 150 then -- Legends
		if string.find(card.name, "%(ital%.?%)") then
			card.lang = { nil,nil,nil,nil,[5] = "ITA" }
			card.name = string.gsub( card.name, "%(ital%.?%)", "(DROP italian)" )
		end
	end -- if 150

	if foiltweak[setid] and foiltweak[setid][card.name] then -- foiltweak
		card.foil = foiltweak[setid][card.name].foil
		if DEBUG then 
			llog("FOILTWEAKed to "  .. card.foil ,2)
		end
	end -- if foiltweak

	return card
end -- function

